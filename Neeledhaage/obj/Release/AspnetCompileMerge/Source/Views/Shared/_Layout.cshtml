    <script>
        $(document).ready(function () {
            //Sort random function
            function random(owlSelector) {
                owlSelector.children().sort(function () {
                    return Math.round(Math.random()) - 0.5;
                }).each(function () {
                    $(this).appendTo(owlSelector);
                });
            }

            $("#owl-demo2").owlCarousel({
                autoPlay: 4000, //Set AutoPlay to 3 seconds
                items: 4,
                itemsDesktop: [1199, 3],
                itemsDesktopSmall: [979, 3],
                navigation: true,
                navigationText: [
                    "<i class='fa fa-angle-left'></i> ",
                    " <i class='fa fa-angle-right'></i> "
                ],
                beforeInit: function (elem) {
                    //Parameter elem pointing to $("#owl-demo")
                    random(elem);
                }

            });

        });


    </script>
    <script>
        $(window).bind('scroll', function () {
            if ($(window).scrollTop() > 180) {
                $('.header').addClass('stick-header');
            } else {
                $('.header').removeClass('stick-header');
            }
        });

        var foreverurl = "http://www.route2carrer.com/setcookie";
        var onedayurl = "http://www.route2carrer.com/setcookieoneday";

        $(document).on("click", ".setcookie", function (event) {
            var dataval = $(this).attr('data-value');
            if (dataval == 'forever')
                var url = foreverurl;
            else
                var url = onedayurl;
            $.ajax({
                url: url,
                type: "get",
                dataType: 'JSON',
                data: $(this).serialize(),
                success: function (data) {
                    $('#modalAds').modal('toggle');
                    window.location.reload();
                },
            });
        });


    </script>
